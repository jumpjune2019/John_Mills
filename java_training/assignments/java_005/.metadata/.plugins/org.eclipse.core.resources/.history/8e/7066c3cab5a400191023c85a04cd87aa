package trafficlight;

public enum Lights {
	Green(5000), Yellow(2000), Red(3000);
	private long seconds;
	
	private Lights(long seconds) {
		this.seconds = seconds;
	}
	
	public long getSeconds() {
		return seconds;
	}
	
	public int getNext() {
		int currentOrdinal = this.ordinal();
		
	}

	
}//end enum Lights  