package gridlayoutchallenge;

public class Driver {

	
	public static void main(String[] args) {
		
		
		SwingUtilities.invokeLater(new Runnable() {
			public void run() {
				MyGrid gridLayoutDemo = new MyGrid();  
			    gridLayoutDemo.showGridLayoutDemo();
			}
		});   
		
		
		
	}//end main
	
	
}//end Driver class