import java.awt.event.WindowAdapter;
import java.awt.event.WindowEvent;

import javax.swing.*;

public class DriverGui {

	JFrame frm;
	JPanel panel;
	JLabel pushButton;
	JLabel showLabel;
	JButton reset;
	ButtonImage fungi;
	ButtonImage dairy;
	
	
	public DriverGui() {
	
		frm = new JFrame("Using Images with buttons");
		frm.setSize(250, 350);
		
		panel = new JPanel(new BoxLayout(panel, BoxLayout.Y_AXIS));
		
		pushAButton = new JLabel("Push a Button");
		
		
		
		
		frm.setVisible(true);
		
		frm.addWindowListener(new WindowAdapter() {
			public void windowClosing(WindowEvent windowEvent) {
				System.exit(0);
			}
		});
		
	}//end DriverGui
	
}//end DriverGui class