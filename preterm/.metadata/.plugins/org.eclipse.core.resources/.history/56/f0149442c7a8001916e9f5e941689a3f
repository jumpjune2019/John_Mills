
public class Seat {

	String rowIndex;
	int seatIndex;
	Client currentClient;
	
	
	public Seat() {
		
	}
	
	
	
	public void setRowIndex(String str) {
		rowIndex = str;
	}
	
	public String getRowIndex() {
		return rowIndex;
	}
	
	public void setSeatIndex(int num) {
		seatIndex = num;
	}
	
	public int getSeatIndex() {
		return seatIndex;
	}
	
}